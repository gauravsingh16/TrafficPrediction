{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"Timestamp",
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack",
			"Packets"
		],
		"cols":	[
			"Packets"
		],
		"sequence_length": 50,
		"train_test_split": 0.70
	},
	"training": {
		"epochs": 200,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "LSTM/saved_models",
		"layers": [
			{
				"type": "LSTM",
				"neurons": 64,
				"input_timesteps": 50,
				"input_dim": 5,
				"return_seq": true,
				"activation": "relu"
			},
			{
				"type": "dropout",
				"rate": 0.5
			},
			{
				"type": "LSTM",
				"neurons": 8,
				"return_seq": true,
				"activation": "relu"
			},
			{
				"type": "dense",
				"neurons": 5,
				"activation": "linear"
			}
		]
	}
}