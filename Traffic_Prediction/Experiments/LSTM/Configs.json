{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"SrNo",
			"Timestamp",
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack"
		],
		"cols":	[
			"Packets"
		],
		"train_test_split": 0.80
	},
	"training": {
		"epochs": 50,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "LSTM/saved_models",
		"layers": [
			{
				"type": "LSTM",
				"neurons": 256,
				"input_timesteps": 49,
				"input_dim": 1,
				"return_seq": true,
				"activation": "relu"
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 128,
				"return_seq": true,
				"activation": "relu"
			},
			{
				"type": "dense",
				"neurons": 50
			},
			{
				"type": "dense",
				"neurons": 1
			}
		]
	}
}