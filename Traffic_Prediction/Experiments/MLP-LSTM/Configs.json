{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"SrNo",
			"Timestamp",
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack"
		],
		"cols":	[
			"Packets"
		],
		"train_test_split": 0.80
	},
	"training": {
		"epochs": 50,
		"batch_size": 16
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "MLP_LSTM/saved_models",
		"layers": [{
			"type": "lstm",
			"neurons": 128,
			"input_timesteps": 49,
			"input_dim": 1,
			"return_seq": true,
			"activation": "relu"
		},
		{
			"type": "LSTM",
			"neurons": 64,
			"input_timesteps": 49,
			"input_dim": 1,
			"return_seq": false,
			"activation": "relu"
		},
		{
			"type": "repeatVector",
			"rate": 1
		},
		{
			"type": "LSTM",
			"neurons": 64,
			"input_timesteps": 49,
			"input_dim": 1,
			"return_seq": true,
			"activation": "relu"
		},
		{
			"type": "LSTM",
			"neurons": 128,
			"input_timesteps": 49,
			"input_dim": 1,
			"return_seq": true,
			"activation": "relu"
		},
		{
			"type": "dense",
			"neurons": 1,
			"activation": "linear"
		}
		]
	}
}