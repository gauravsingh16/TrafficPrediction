{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack",
			"Packets"
		],
		"cols":	[
			"Packets"
		],
		"sequence_length":2,
		"train_test_split": 0.80
	},
	"training": {
		"epochs": 50,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "AE_LSTM/saved_models",
		"layers": [{
			"type": "lstm",
			"neurons": 64,
			"input_timesteps": 2,
			"input_dim": 5,
			"return_seq": false,
			"activation": "relu"
		},
		{
			"type": "repeatVector",
			"repeatvector_rate": 2
		},
		{
			"type": "lstm",
			"neurons": 64,
			"input_timesteps": 2,
			"input_dim": 5,
			"return_seq":true,
			"activation": "relu"
		},
		{
			"type":"dropout",
			"rate": 0.8
		},
		{
			"type": "Dense",
			"neurons": 5,
			"activation": "linear"
		}
		]
	}
}