{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack",
			"Connectivity",
			"Seconds",
			"Minutes"		
		],
		"cols":	[
			"Packets"
		],
		"sequence_length": 1,
		"train_test_split": 0.80
	},
	"training": {
		"epochs": 100,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "AE_LSTM/saved_models",
		"layers": [{
			"type": "lstm",
			"neurons": 16,
			"input_timesteps": 1,
			"input_dim": 6,
			"return_seq": true,
			"activation": "relu"
		},
		{
			"type":"dropout",
			"rate": 0.8
		},
		{
			"type": "lstm",
			"neurons": 8,
			"return_seq": false,
			"activation": "relu"
		},
		{
			"type":"dropout",
			"rate": 0.8
		},
		{
			"type": "repeatVector",
			"repeatvector_rate": 1
		},
		{
			"type": "lstm",
			"neurons": 8,
			"return_seq": true,
			"activation": "relu"
		},
		{
			"type":"dropout",
			"rate": 0.8
		},
		{
			"type": "lstm",
			"neurons": 16,
			"input_timesteps": 1,
			"input_dim": 6,
			"return_seq":true,
			"activation": "relu"
		},
		{
			"type":"dropout",
			"rate": 0.8
		},
		{
			"type": "Dense",
			"neurons": 2,
			"activation": "linear"
		}
		]
	}
}