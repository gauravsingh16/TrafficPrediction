{
	"data": {
		"filename": "/home/gaurav/TrafficPrediction/dataNetwork.csv",
		"columns": [
			"TimeStamp",
			"Sender's IP",
            "Receiver's IP",
            "Protocol Stack"
		],
		"cols":	[
			"Packets"
		],
		"sequence_length": 10000,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 2,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 128,
				"input_timesteps": 49,
				"input_dim": 2,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 2,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 2,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"return_seq": true,
                "activation": "linear"
			}
		]
	}
}